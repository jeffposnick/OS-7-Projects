program Multiplication(input, output);const	maxCol = 10;	maxRow = 10;	colSpace = 4;var	i, j: integer;	begin	write('       X |');	for i := 1 to maxCol do		write(i:colSpace);	writeln;	write('      ');	for i := 1 to maxCol*colSpace+4 do		write('-');	writeln;	for j := 1 to maxRow do begin		write(j);		write(' |');		for i := 1 to maxCol do			write(i*j:colSpace);		writeln;	end;	end.	